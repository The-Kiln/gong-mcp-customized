{
  "name": "gong-api--subset-",
  "version": "0.1.0",
  "description": "MCP Server generated from OpenAPI spec for gong-api--subset-",
  "private": true,
  "type": "module",
  "main": "build/index.js",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "start": "node server.js",
    "start:enhanced": "node enhanced-server.js",
    "start:improved": "node improved-server.js",
    "start:simple": "node simplified-mcp.js",
    "start:simple-v2": "node simplified-mcp-v2.js",
    "start:mcp": "node build/index.js",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prestart": "npm run build",
    "prestart:enhanced": "npm run build",
    "prestart:improved": "npm run build",
    "test": "jest",
    "test:smoke": "jest tests/gongSmoke.test.ts",
    "clean": "rimraf build",
    "rebuild": "npm run clean && npm run build",
    "lint": "eslint 'src/**/*.ts'",
    "dev": "tsc -w"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.0",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "json-schema-to-zod": "^2.6.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.2",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3"
  }
}